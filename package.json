{
    "name": "vsBackground",
    "version": "0.0.3",
    "displayName": "vsBackground",
    "description": "%vsbackground.description%",
    "-----------------------------------------[ author ]-----------------------------------------": "",
    "publisher": "cosy247",
    "author": {
        "name": "cosy247",
        "email": "705194673@qq.com",
        "url": "https://cosy247.github.io"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/cosy247/VscodePlugin-Background.git"
    },
    "bugs": {
        "url": "https://github.com/cosy247/VscodePlugin-Background/issues"
    },
    "-----------------------------------------[ info ]-----------------------------------------": "",
    "icon": "icon.png",
    "engines": {
        "vscode": "^1.54.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "keywords": [
        "theme",
        "background",
        "customization"
    ],
    "-----------------------------------------[ contributes ]-----------------------------------------": "",
    "main": "extension",
    "contributes": {
        "commands": [
            {
                "command": "changeBackground",
                "title": "VsBackground: Change Background"
            }
        ],
        "configuration": {
            "title": "Background",
            "properties": {
                "vsbackground": {
                    "type": "object",
                    "description": "%vsbackground.showType%",
                    "default": {
                        "showType": "partition",
                        "full": {
                            "img": "",
                            "opacity": 0.1
                        },
                        "titlebar": {
                            "img": "",
                            "opacity": 0.1
                        },
                        "banner": {
                            "img": "",
                            "opacity": 0.15
                        },
                        "activitybar": {
                            "img": "",
                            "opacity": 0.1
                        },
                        "sidebar": {
                            "img": "",
                            "opacity": 0.1
                        },
                        "editor": {
                            "img": "",
                            "opacity": 0.1
                        },
                        "panel": {
                            "img": "",
                            "opacity": 0.1
                        },
                        "auxiliarybar": {
                            "img": "",
                            "opacity": 0.1
                        },
                        "statusbar": {
                            "img": "",
                            "opacity": 0.1
                        }
                    }
                }
            }
        }
    }
}
